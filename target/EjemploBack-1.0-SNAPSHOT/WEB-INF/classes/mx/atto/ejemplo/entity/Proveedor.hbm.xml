<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="mx.atto.ejemplo.entity">
    <class table="proveedores" name="Proveedor">
        <id name="id" type="java.lang.Integer">
            <generator class="native"/>
        </id>
        <property name="codigoProovedores" column="codigo_proovedores" type="string"/>
        <property name="codigoBarrasproveedores" column="codigoBarras_proveedores"  type="string"/>

       <!-- <one-to-one name="idCategoria" column="id_categoria" class="mx.atto.ejemplo.entity.Categoria"/>
-->
        <one-to-one name="tercero"  class="mx.atto.ejemplo.entity.Tercero"/>
    </class>
</hibernate-mapping>
